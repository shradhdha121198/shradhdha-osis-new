<script>
  $('body').click('.header-initial .menu-bar button', function () {
    $('.site-header').addClass('visible');
  });

  $(window).scroll(function () {
    var sd = jQuery(window).scrollTop();
    if (sd > 0) {
      $('.site-header').addClass('sticky');
    } else {
      $('.site-header').removeClass('sticky');
    }
  });
  
  $('.cart-icon .cart-image img').on('click', function(e){
    e.preventDefault();
    var link = $(this).parents('.cart-image').attr('href')+'?view=mini';
    $.get(link, function(data){
      $('.cart_pop').html(data);
    });
    $('.cart_popup').show();
    $.getJSON("/cart.js", function(cart) {
      $('.item_count').text(cart.item_count);
    });
  });
</script>
