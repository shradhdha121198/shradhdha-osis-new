<script>
  /* start =  instant search - means search input na bajuma popup (search nu sort list) */
  $(".search_input").on('focus, blur, keyup',function(e){
    var value=$(this).val();
    $.getJSON("/search/suggest.json", {
      "q": value,
      "resources": {
        "type": "product,page,article,collection"
      }
    }).done(function(data){
      var result="",index=0,results=[],results_div="<div class='display'>";
      for(var item in data.resources.results){
        for(var i in data.resources.results[item]){
          if(item == 'articles'){
            if(data.resources.results[item][i].image){
              results_div+="<div class='row search_header'><div class='image'><a class='image_not_a' href='"+data.resources.results[item][i].url+"'><img  loading='lazy' src='"+data.resources.results[item][i].image+"' ></a></div>";
            }
            results_div+="<div class='desc_search'><a href='"+data.resources.results[item][i].url+"'><h2 class='search_pro'>"+data.resources.results[item][i].title+"</h2></a>\
Author:"+data.resources.results[item][i].author+"\
  </div></div>";
          }
          else if(item == 'products') {
            if(data.resources.results[item][i].featured_image){
              results_div+="<div class='row search_header'><div class='image'><a class='image_not_a' href='"+data.resources.results[item][i].url+"'><img loading='lazy' src='"+data.resources.results[item][i].featured_image.url+"' ></a></div>";
            }
            results_div+="<div class='desc_search'><a href='"+data.resources.results[item][i].url+"'><h2 class='search_pro'>"+data.resources.results[item][i].title+"</h2></a>\
<p>Price: "+data.resources.results[item][i].price+" </p>\
  </div></div>";              
          }
          else if(item == 'pages'){
            results_div+="<div class='row search_header'><div class='desc_search_page'><a href='"+data.resources.results[item][i].url+"'><h2 class='search_pro'>"+data.resources.results[item][i].title+"</h2></a>\
<p>"+data.resources.results[item][i].body.slice(0, 50).concat('...')+"</p>\
  </div></div>";
          }
          else if(item == 'collections'){
            if(data.resources.results[item][i].featured_image){
              results_div+="<div class='row search_header'><div class='image'><a class='image_not_a' href='"+data.resources.results[item][i].url+"'><img loading='lazy' src='"+data.resources.results[item][i].featured_image.url+"' ></a></div>";
            }
            results_div+="<div class='desc_search'><a href='"+data.resources.results[item][i].url+"'><h2 class='search_pro'>"+data.resources.results[item][i].title+"</h2></a>\
  </div></div>";
          }
        }
      }
      $('.search_popup').html(results_div);
      $('.search_popup').show();
      $('.overlay_search').css('display','block');
      $(".search_popup .remove").on("click",function(e){
        $('.search_popup').hide();
        $('.overlay_search').hide();
      });
    }); 
    if (value == ''){
      $('.overlay_search').css('display','none');
      $('.search_popup').css('display','none');
    }
  });
  /* end =  instant search - means search input na bajuma popup (search nu sort list) */
</script>
