  <div class="main-header">
    <div class="container">
      <div class="row ai-c">
        <div class="menu-bar">
          <button class="menu-opener"><img  loading="lazy" src="https://cdn.shopify.com/s/files/1/0427/3936/8087/files/menu-icon.svg?v=1610947757" alt="Menu"></button>
        </div>
        {% if section.settings.image_logo != blank %}
        <div class="logo">
          <a href="{{ shop.url }}"><img  loading="lazy" src="{{ section.settings.image_logo | img_url: "master" }}" alt="Oasis"></a>
        </div>
        {% endif %}

        <nav class="nav">
          <ul class="navigation">
            {% assign l = section.settings.main_menu %}
            {% for link in linklists[l].links  %}
              {% if linklists[link.handle] == empty %}
              {% assign hasmega = false %}
                {% for block in section.blocks %}
                {% assign mmi = block.settings.menuItem | downcase | handle %}
                {% assign li = link.title | downcase | handle %}
                  {% if li == mmi %}
                  {% assign hasmega = true %}
                  {% assign x = forloop.index %}
                  {% break %}
                  {% endif %}
                {% endfor %}
              {% endif %}
              <li class="{% if hasmega %}mega-menu{% endif %} nav-item {% if link.title == 'CONTACT' %}nav-item-contact{% endif %}" data-item="{{ link.title | downcase | handle  }}">
                <a href="{{ link.url }}" class="nav-link" style="font-family: DIN-Bold;">{{ link.title }}</a>
                {% if link.links != blank %}
                <ul class="submenu">
                  {% for child_link in link.links %}
                  <li>
                    <a href="{{ child_link.url }}" class="nav-link" style="font-family: DIN-Bold;"> {{ child_link.title }}</a>
                    {% if child_link.links != blank %}
                    <ul class="submenu_1">
                      {% for grandchild_link in child_link.links %}
                      <li>
                        <a href="{{ grandchild_link.url }}" class="nav-link" style="font-family: DIN-Bold;">
                          {{ grandchild_link.title }}
                        </a>
                      </li>
                      {% endfor %}
                    </ul>
                    {% endif %}
                  </li>
                  {% endfor %}
                </ul>
                {% endif %}
                {%- if hasmega -%}
                  {%- assign x = x | minus: 1 -%}
                  {%- assign b = section.blocks[x] -%}
                  <div class="mega-menu-container {{ b.settings.menuItem | downcase | handle  }}" data-item="{{ b.settings.menuItem | handle }}">
                    <div class="container">
                      <div class="row">
                        {% if b.type == 'manu' %}
                        <div class="mega-menu-col" >
                          {% assign m1 = b.settings.menu1 %}
                          <ul class="m-menu">
                            {% for l in linklists[m1].links %}
                            <li><a href="{{ l.url }}">{{ l.title }}</a></li>
                            {% endfor %}
                          </ul>
                        </div>
                        {% if b.settings.bannerlink != blank %}
                        <div class="mega-menu-col m-img-col" >
                          <div class="m-block">
                            <a href="{{ b.settings.bannerlink }}">
                              <img  loading="lazy" src="{{ b.settings.banner | img_url: 'large' }}" alt="" />
                            </a>
                            <span class="m-block-content">{{ b.settings.text }}</span>
                          </div>
                        </div>
                        {% endif %}
                        {% if b.settings.bannerlink1 != blank %}
                        <div class="mega-menu-col m-img-col" >
                          <div class="m-block">
                            <a href="{{ b.settings.bannerlink1 }}">
                              <img  loading="lazy" src="{{ b.settings.banner1 | img_url: 'large' }}" alt="" />
                            </a>
                            <span class="m-block-content">{{ b.settings.text1 }}</span>
                          </div>
                        </div>
                        {% endif %}
                        {% endif %}
                      </div>
                    </div>
                </div>
                {% endif %}
              </li>
            {% endfor %}
          </ul>
        </nav>
        
        <!-- start code for multiplae currency -->
        {% form 'currency' , class:'currency' %}
        <select name="currency" id="select_currency">
          {% for currency in shop.enabled_currencies %}
          {% if currency == cart.currency %}
          <option selected="true" value="{{currency.iso_code}}">{{currency.iso_code}} {{currency.symbol}}</option>
          {% else %}
          <option value="{{currency.iso_code}}">{{currency.iso_code}}</option>
          {% endif %}
          {% endfor %}
        </select>
        <input type="hidden" name="form_type" value="currency">
        <input type="hidden" name="return_to" value="{{ request.path }}">
        {% endform %}
        <script>
          $('body').on("change","#select_currency",function(e){
            $('.currency').submit();
          });
        </script>
        <!-- end code for multiplae currency -->

        {% comment %}
        <!-- start code for multiplae currency (JS- selectbox convert ul/li) and Gliss code -->
        {% form 'currency', class: "currency" %}
        <select name="currency" id="select_currency" class="select_currency_class">
          {% for currency in shop.enabled_currencies %}
          {% if currency == cart.currency %}
          <option selected="true" value="{{currency.iso_code}}">{{currency.iso_code}} {% if currency.symbol %}{{ currency.symbol }}{% endif %}</option>
          {% else %}
          <option value="{{currency.iso_code}}">{{currency.iso_code}} {% if currency.symbol %}{{ currency.symbol }}{% endif %}</option>
          {% endif %}
          {% endfor %}
        </select>
        <input type="hidden" name="form_type" value="currency">
        <input type="hidden" name="return_to" value="{{ request.path }}">
        {% endform %}
        <script>
          $('body').on("change","#select_currency",function(e){
            $(this).closest('form').submit();
          });

          $('#select_currency').each(function() {
            var select = $(this),
                name = select.attr('name');
            select.hide();
            select.wrap('<div class="nice-select-wrap"></div>');
            var parent = select.parent('.nice-select-wrap');
            parent.append('<ul id=' + name + ' style="display:none"></ul>');
            select.find('option').each(function() {
              var option = $(this),
                  value = option.attr('value'),
                  label = option.text();
              if (option.is(":selected")) {
                $('<a href="#" class="drop">' + label + '</a>').insertBefore(parent.find('ul'));
              } else {
                parent.find('ul').append('<li><a href="#" id="' + value + '">' + label + '</a></li>');
              }
            });
            parent.find('a').on('click', function(e) {
              parent.toggleClass('down').find('ul').slideToggle(300);
              e.preventDefault();
            });
            parent.find('ul a').on('click', function(e) {
              e.preventDefault();
              var niceOption = $(this),
                  value = niceOption.attr('id'),
                  text = niceOption.text();
              select.val(value);
              parent.find('.drop').text(text);
              $('#select_currency').val(value).change();
            });
          });
        </script>
        <!-- end code for multiplae currency -->
        {% endcomment %}
        
        
        
        
